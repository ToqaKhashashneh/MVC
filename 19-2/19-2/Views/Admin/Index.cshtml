
@{
    Layout = "Admin";
}


@{
    string[] UserName = { "Toqa", "Reem", "Haneen" };
    int[] Age = { 25, 15, 29 };
    string[] Email = { "Toqa@gmail.com", "Reem@gmail.com", "Haneen@gmail.com" };
}

<table class="table">
    <thead class="thead-dark">
        <tr>

            <th scope="col">User Name</th>
            <th scope="col">Age</th>
            <th scope="col">Email</th>
        </tr>
    </thead>
    <tbody>
        @for (int i = 0; i < UserName.Length; i++)
        {
            <tr>
                <td>@UserName[i]</td>
                <td>@Age[i]</td>
                <td>@Email[i]</td>
            </tr>
        }
    </tbody>
</table>